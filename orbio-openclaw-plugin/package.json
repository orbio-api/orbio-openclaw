{
  "name": "@orbio/orbio-openclaw",
  "version": "0.1.0",
  "description": "Official Orbio tools for OpenClaw.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "skills",
    "README.md"
  ],
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbio/orbio-openclaw.git",
    "directory": "orbio-openclaw-plugin"
  },
  "bugs": {
    "url": "https://github.com/orbio/orbio-openclaw/issues"
  },
  "homepage": "https://github.com/orbio/orbio-openclaw/tree/main/orbio-openclaw-plugin#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "node -e \"const fs=require('fs'); fs.rmSync('dist',{recursive:true,force:true});\"",
    "build": "pnpm clean && tsc -p tsconfig.build.json",
    "lint": "eslint \"{src,tests}/**/*.ts\" --max-warnings=0",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "smoke:live": "pnpm build && node scripts/live-smoke.mjs",
    "quality": "pnpm lint && pnpm typecheck && pnpm coverage",
    "typecheck": "tsc --noEmit",
    "verify": "pnpm quality && pnpm build",
    "prepack": "pnpm build"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.41"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@types/node": "^22.13.4",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "openclaw",
    "orbio",
    "plugin",
    "whatsapp",
    "telegram",
    "slack",
    "lead-generation",
    "account-discovery"
  ]
}
